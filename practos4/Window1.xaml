<Window x:Class="practos4.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:practos4"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition/>
            <RowDefinition/>
            

        </Grid.RowDefinitions>
        <Button x:Name="exit" Content="Выйти" Click="MainWind_Click"/>
        <Button x:Name="Redact" Content="Редактор теста" Grid.Column="1" Click="Redact_Click" IsEnabled="False"/>
        <Button x:Name="Test" Content="Пройти тест" Grid.Column="2" Click="Test_Click"/>
        <Frame x:Name="Page" Grid.Column="0" NavigationUIVisibility="Hidden" Grid.Row="1" Grid.ColumnSpan="3"/>



    </Grid>
</Window>
